/*
次のコードに追加し、九九表を表示するプログラムを完成させなさい。
*/

#include <stdio.h>

int main(void) {
    int kuku[9][9];
    int i, j;

    // 配列 kuku に値を代入する

    for (i = 0; i < 9 ;i++) {
        for (j = 0; j < 9;j++) {
            kuku[i][j] = (i+1) * (j+1);
        }
    }

    for (i = 0; i < 9; i++)
    {
        for (j = 0; j < 9; j++)
            printf(" %2d", kuku[i][j]);

        printf("\n");
    }

    return 0;
}
