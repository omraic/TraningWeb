/*
整数を 10 回入力し、小さい順に並べ替えて表示するプログラムを作成しなさい。
*/

#include <stdio.h>

int main(void) {
    int num[10] = { 0 };

    printf("数値を10回入力してください。\n");

    for (int i = 0; i < 10;i++) {
        scanf_s("%d",&num[i]);
    }

    for (int i = 0; i < 10;i++) {
        int e = 0;
            if (num[i] > num[i + 1]) {
                e = num[i];
                num[i] = num[i + 1];
                num[i + 1] = e;

            }
            
    }

    for (int i = 0; i < 10;i++) {
        printf("%d ",num[i]);
    }

    return 0;
}
