/*
次のプログラムを作成しなさい。

文字列を入力する。
入力された文字列を逆順に表示する。
*/

#include <stdio.h>

int main(void) {
	char    str[100];
	
	scanf_s("%s", str,100);

	int count = 0;
	for (int i = 0; i < 100;i++) {
		if(str[i]!='\0') {
			count++;
		}
		else {
			break;
		}

	}
	count--;

	for (int i =count ; i >=0;i--) {
		printf("%c",str[i]);
	}




	return 0;
}
